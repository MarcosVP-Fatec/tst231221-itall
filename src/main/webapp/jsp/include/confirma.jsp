<div class="modal fade" id="modalConfirma" tabindex="-1" role="dialog" aria-labelledby="modalConfirmaTit" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="modalConfirmaTit"></h5>
      </div>
      <div class="modal-body""><span id="modalConfirmaTexto"></span></div>
      <div class="modal-footer">
        <button type="button" id="modalConfirmaSim" class="btn btn-success" onmouseup="$('#modalConfirma').modal('hide')">Sim</button>
        <button type="button" id="modalConfirmaNao" class="btn btn-danger"  onmouseup="$('#modalConfirma').modal('hide')" data-dismiss="modal">Não</button>
      </div>
    </div>
  </div>
</div>